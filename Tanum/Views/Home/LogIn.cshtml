@model Tanum.Models.UserModel@{ ViewData["Title"] = "Index";}@{ Layout = null;}<head>    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous" />    <link rel="stylesheet" href="style.css" />    <style>        body {            background-color: #bb333f;            font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;            font-size: 0,75em !important;        }        form {            padding: 70px;            background: #fff;        }        input[type=text], input[type=password] {            width: 100%;            padding: 12px 20px;            margin: 8px 0;            display: inline-block;            border: 1px solid #ccc;            box-sizing: border-box;        }        #login {            width: 40%;            padding: 12px 20px;            margin: 8px 0;            display: inline-block;            border: 1px solid #ccc;            box-sizing: border-box;            background-color: #bb333f;            float: right;            color: white;        }        button:hover {            opacity: 0.8;        }        .formsize {            width: 500px;            height: 300px;        }    </style></head>@*@using (Html.BeginForm()){<form>    AnvändarNamn: @Html.EditorFor(A => A.username)    <br />    <br />    Lösenord:     @Html.EditorFor(A => A.password)    <br />    <button type="submit" class="btn btn-primary mt-3" name="login">Logga in</button></form>}*@<div class="container py-5">    <div class="row justify-content-center">        <div class="col-lg-6">            <div class="card shadow formsize">                <h2 class="text-center mb-0 mt-5"><img src="~/Content/Tanum_farg_liggande.gif" alt=""></h2>                <form action="Index">                    <div class="form-group">                        <label for="Anvandarnamn" class="control-label" style="color:#adadad">Användarnamn</label>                        <input type="text" id="Anvandarnamn" class="form-control" />                    </div>                    <div class="form-group">                        <label for="Losenord" class="control-label" style="color: #adadad">Lösenord</label>                        <input type="password" id="Losenord" class="form-control" />                    </div>                    <div class="form-group">                        <input type="submit" value="Logga in" id="login" />                    </div>                    <p>@ViewBag.ErrorMessage</p>                </form>            </div>        </div>    </div></div>